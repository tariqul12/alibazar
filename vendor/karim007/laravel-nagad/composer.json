            "Karim007\\LaravelNagad\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Md abdul karim",
            "email": "karim.cse007@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
